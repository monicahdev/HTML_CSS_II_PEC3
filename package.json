{
  "name": "ecocircular",
  "version": "1.0.0",
  "private": true,
  "description": "Website based on UOC-Boilerplate using Tailwind",
  "scripts": {
    "parcel:serve": "parcel -p 8123 --target web --open",
    "parcel:build": "parcel build --target web --no-source-maps --no-cache",
    "clean": "rimraf dist .cache .cache-loader .parcel-cache",
    "dev": "npm-run-all clean parcel:serve",
    "build": "npm-run-all clean parcel:build",
    "test": "echo 'Everything is working as expected ✅\nStart working on your project by running \\033[1mnpm run dev\\033[0m'"
  },
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "./",
      "source": "src/index.html",
      "engines": {
        "browsers": "last 2 versions, > 0.5%, not dead"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monicahdev/HTML_CSS_II_PEC3"
  },
  "author": {
    "name": "Mónica Hernández"
  },
  "license": "MIT",
  "homepage": "https://github.com/monicahdev/HTML_CSS_II_PEC3",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.13.3",
    "@tailwindcss/postcss": "^4.1.8",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.13.3",
    "postcss": "^8.5.4",
    "postcss-preset-env": "^10.1.3",
    "posthtml-include": "^2.0.1",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.1.8"
  }
}
